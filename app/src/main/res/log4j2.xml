<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <LogCat name="LogCatAppender" target="System.out">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </LogCat>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="LogCatAppender"/>
        </Root>
    </Loggers>
</Configuration>
